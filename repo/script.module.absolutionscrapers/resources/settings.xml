    <category label="Scraper Accounts">
        <setting type="lsep" label="Easynews" />
        <setting id="provider.easynews" type="bool" label="EASYNEWS" default="false" />
        <setting id="easynews.user" type="text" subsetting="true" label="Username" default="" visible="eq(-1,true)" />
        <setting id="easynews.pass" type="text" subsetting="true" label="Password" default="" visible="eq(-2,true)" />
        <setting id="easynews.title.chk" type="bool" subsetting="true" label="Title filter" default="true" visible="eq(-3,true)" />
