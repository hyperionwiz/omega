        <setting id="install.orion" subsetting="true" type="action" label="To use this you must install and configure an add-on: Orion.[CR]    Account required." option="close" action="InstallAddon(script.module.orion)" enable="!System.HasAddon(script.module.orion)" visible="eq(-1,true)"/>
